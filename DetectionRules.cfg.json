{
  "Regex": [
    {
      "Name": "IPv4Address",
      "Pattern": "(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)",
      "Confidence": "High",
      "IsActive": true,
      "AppliesTo": [
        {
          "SourceType": "Log",
          "SourceFilter": "access.log"
        }
      ],
      "Tags": [
        "Network",
        "Audit"
      ]
    },
    {
      "Name": "IPv6Address",
      "Pattern": "^(?:[A-Fa-f0-9]{1,4}:){7}[A-Fa-f0-9]{1,4}$",
      "Confidence": "Low",
      "IsActive": true,
      "AppliesTo": [
        {
          "SourceType": "Log",
          "SourceFilter": "access.log"
        }
      ],
      "Tags": [
        "Network",
        "Audit"
      ]
    },
    {
      "Name": "DistinguishedName",
      "Pattern": "^([a-zA-Z0-9\\-]+=[^,]+,?)+$",
      "Confidence": "Low",
      "IsActive": true,
      "AppliesTo": [
        {
          "SourceType": "File",
          "SourceFilter": ".ldif"
        }
      ],
      "Tags": [
        "Identity",
        "LDAP"
      ]
    },
    {
      "Name": "GUID",
      "Pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$",
      "Confidence": "Low",
      "IsActive": true,
      "AppliesTo": [
        {
          "SourceType": "File",
          "SourceFilter": ".txt"
        }
      ],
      "Tags": [
        "Identifier",
        "UUID"
      ]
    }
  ]
}
